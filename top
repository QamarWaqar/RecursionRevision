[32m+[m[32mï»¿using System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Threading.Tasks;[m
[32m+[m
[32m+[m[32mnamespace RecusionRevision[m
[32m+[m[32m{[m
[32m+[m[32m    class Program[m
[32m+[m[32m    {[m
[32m+[m[32m        static int sI = 0;[m
[32m+[m[32m        static void Main(string[] args)[m
[32m+[m[32m        {[m
[32m+[m[32m            Console.WriteLine("Calling Object Recursion");[m
[32m+[m[32m            Program p = new Program();[m
[32m+[m[32m            p.func();[m
[32m+[m[32m            // Now Caliing Static Recursion //[m
[32m+[m[32m            sI = 0;[m
[32m+[m[32m            Console.WriteLine("Now Calling Static Recursion Function");[m
[32m+[m[32m            dunc();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        void func()[m
[32m+[m[32m        {[m
[32m+[m[32m            if (sI == 3) { return; }[m
[32m+[m[32m            if (sI >= 0) { Program p = new Program(); sI++; Console.WriteLine("Calling"); p.func(); Console.WriteLine("Returning"); }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        static void dunc()[m
[32m+[m[32m        {[m
[32m+[m[32m            if (sI == 3) { return; }[m
[32m+[m[32m            if (sI >= 0) { sI++; Console.WriteLine("Calling"); dunc(); Console.WriteLine("Returning"); }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
